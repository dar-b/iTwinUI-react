[
  {
    "replace": "rotate(0deg)",
    "with": "rotate(360deg)"
  },
  {
    "replace": "var(--iui-xl)",
    "with": "var(--iui-size-xl)"
  },
  {
    "replace": "var(--iui-m)",
    "with": "var(--iui-size-m)"
